-- Main-Mine

local mine = Main:Channel("Mine")

mine:Button("Buy Cash Crystals", function()
    for count = 1, 25 do
        local args = {
            [1] = 0,
            [2] = "Green"
        }
       
        game:GetService("ReplicatedStorage").AddCrystal:FireServer(unpack(args))
        wait()
    end
end)

mine:Button("Buy View Crystals", function()
    for count = 1, 25 do
        local args = {
            [1] = 0,
            [2] = "Pink"
        }
       
        game:GetService("ReplicatedStorage").AddCrystal:FireServer(unpack(args))
        wait()
    end
end)

mine:Button("Buy Follower Crystals", function()
    for count = 1, 25 do
        local args = {
            [1] = 0,
            [2] = "Red"
        }
       
        game:GetService("ReplicatedStorage").AddCrystal:FireServer(unpack(args))
        wait()
    end
end)

mine:Button("Buy Byte Crystals", function()
    for count = 1, 25 do
        local args = {
            [1] = 0,
            [2] = "Purple"
        }
       
        game:GetService("ReplicatedStorage").AddCrystal:FireServer(unpack(args))
        wait()
    end
end)


-- Main-AutoFarm


local AF = Main:Channel("Auto Farm")

AF:Toggle("Auto-Farm",false, function(v)
    getgenv().Autofarm = v
    while true do
        if not getgenv().Autofarm then return end
        local args = {
        [1] = 99999999,
        [2] = 99999999,
        [3] = -150,
        [4] = 9999999,
        [5] = 9
        }
        
        game:GetService("ReplicatedStorage").StreamEnd:FireServer(unpack(args))
        wait()
    end
end)


-- Main-Misc


local misc = Main:Channel("Misc")

misc:Button("Anti-Afk", function()
    local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:connect(function()
        vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        wait(1)
        vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
end)
