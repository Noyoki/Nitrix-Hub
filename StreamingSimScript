    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(846.479797, 91.9859772, -0.441253871, -0.0409136601, -2.12845425e-05, -0.999162674, 5.3258475e-07, 1, -2.13241874e-05, 0.999162674, -1.4045894e-06, -0.0409136601)
