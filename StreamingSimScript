    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(365.721649, 91.9862595, 0.946885169, 0.109656483, -2.83660556e-05, -0.99396956, -2.34947617e-08, 1, -2.85407459e-05, 0.99396956, 3.15303078e-06, 0.109656483)
