local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/discord%20lib.txt")()

local win = DiscordLib:Window("Streaming Simulator")


-- Main


local Main = win:Server("Main", "http://www.roblox.com/asset/?id=6031075938")


-- Main-Player


local Player = Main:Channel("Player")

local sldr = Player:Slider("Walkspeed", 16, 500, 0, function(v)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
end)

local sldr = Player:Slider("Jump Height", 50, 250, 0, function(v)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
end)


-- Main-Visuals


local visuals = Main:Channel("Visuals")

visuals:Label("Performance")

visuals:Button("Remove Depth Of Field", function()
    game.Lighting.Blur:Remove()
    game.Lighting.Bloom:Remove()
    game.Lighting.DepthOfField:Remove()
end)

visuals:Button("Remove Textures", function()
    local descendants = game.Workspace:GetDescendants()
    
    for index, descendant in pairs(descendants) do
        if descendant:IsA("Texture") then
            descendant:Remove()
        end
    end
end)

visuals:Button("Remove Particles", function()
    local descendants = game.Workspace:GetDescendants()
    
    for index, descendant in pairs(descendants) do
        if descendant:IsA("ParticleEmitter") then
            descendant:Remove()
        end
    end
end)

visuals:Button("Toggle Shadows", function()
    local lighting = game.Lighting
    
    if lighting.GlobalShadows == true then
        lighting.GlobalShadows = false
    else
        lighting.GlobalShadows = true
    end
end)


-- Main-TP


local TP = Main:Channel("Teleports")

TP:Label("Sections")

local FirstSec = TP:Button("First Section", function()
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(101.583344, 96.7285385, 9.23076344, 0.0846221521, -4.02222104e-05, -0.996413112, 1.07614073e-06, 1, -4.02756086e-05, 0.996413112, 2.33592777e-06, 0.0846221521)
end)

